{
  "name": "posekit",
  "description": "Professional pose reference library for artists and designers",
  "type": "dockerfile",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/posekit.git",
    "branch": "main"
  },
  "buildArgs": {
    "NEXT_PUBLIC_API_URL": "${NEXT_PUBLIC_API_URL}",
    "NEXT_PUBLIC_CDN_URL": "${NEXT_PUBLIC_CDN_URL}"
  },
  "environment": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "${NEXT_PUBLIC_API_URL}",
    "NEXT_PUBLIC_CDN_URL": "${NEXT_PUBLIC_CDN_URL}"
  },
  "domains": [
    {
      "host": "your-domain.com",
      "port": 3000,
      "path": "/"
    }
  ],
  "healthCheck": {
    "enabled": true,
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "resources": {
    "limits": {
      "memory": "2Gi",
      "cpu": "1000m"
    },
    "requests": {
      "memory": "512Mi",
      "cpu": "250m"
    }
  },
}